<?xml version="1.0" encoding="UTF-8" ?>

<profiles>
    <original method = "MongoDBService.pullProfile">
        <profile key="name" type="String" readonly="false" verify="true" default="微露用户" description="用户姓名" deprecated="false" />
        <profile key="birthday" type="Long" readonly="false" default=""  description="用户生日" deprecated="false"/>
    </original>

    <original method = "MysqlProfileService.pullProfile">
        <profile key="email" type="string" readonly="true" buildMethod="fetchEmail" description="账号邮箱地址" deprecated="false"/>
    </original>

    <transfer>
        <profile key="age" type="int" readonly="true"  transMethod="birthToAge" description="计算实际年龄"/>
    </transfer>

</profiles>